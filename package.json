{
  "name": "openlayers-prefetching",
  "version": "0.1.3",
  "description": "Prefetching Manager for OpenLayers - An extension to optimize tile loading",
  "main": "dist/PrefetchManager.js",
  "module": "dist/PrefetchManager.esm.js",
  "types": "dist/types/PrefetchManager.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/PrefetchManager.d.ts",
      "import": "./dist/PrefetchManager.esm.js",
      "require": "./dist/PrefetchManager.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.js -w",
    "prepublishOnly": "npm run build",
  "test": "tsx tests/priority-order.test.ts",
    "test:e2e": "npm run build && playwright test",
    "serve": "npx http-server . -p 8080"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "openlayers",
    "prefetch",
    "tiles",
    "map",
    "performance",
    "extension"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "ol": "^7.0.0 || ^8.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.11.30",
    "ol": "^8.0.0",
    "rollup": "^3.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
